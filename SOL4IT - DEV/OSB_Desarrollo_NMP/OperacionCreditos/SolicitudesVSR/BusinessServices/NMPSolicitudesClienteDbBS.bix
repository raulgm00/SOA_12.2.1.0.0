<?xml version="1.0" encoding="UTF-8"?>
<con:businessServiceEntry xmlns:con="http://xmlns.oracle.com/servicebus/business/config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:oper="http://xmlns.oracle.com/servicebus/business/operations" xmlns:ser="http://www.bea.com/wli/sb/services" xmlns:tran="http://www.bea.com/wli/sb/transports" xmlns:env="http://www.bea.com/wli/config/env">
    <con:coreEntry>
        <con1:binding type="SOAP" xsi:type="con:SoapBindingType" isSoap12="false" xmlns:con="http://www.bea.com/wli/sb/services/bindings/config" xmlns:con1="http://xmlns.oracle.com/servicebus/business/config">
            <con:wsdl ref="OperacionCreditos/SolicitudesVSR/Resources/WSDLs/NMPSolicitudesVsrDbWSDL"/>
            <con:binding>
                <con:name>NMPSolicitudesVSRDBSOAP11Binding</con:name>
                <con:namespace>http://servicios.montedepiedad.com.mx/NMP/Service/SolicitudesVSR</con:namespace>
            </con:binding>
            <con:WSI-compliant>false</con:WSI-compliant>
        </con1:binding>
        <oper:operations enabled="true">
            <oper:throttling enabled="false"/>
            <oper:resultCachingEnabled>true</oper:resultCachingEnabled>
        </oper:operations>
        <con:ws-policy>
            <ser:binding-mode>no-policies</ser:binding-mode>
        </con:ws-policy>
        <con:xqConfiguration>
            <ser:snippetVersion>1.0</ser:snippetVersion>
        </con:xqConfiguration>
    </con:coreEntry>
    <con:endpointConfig>
        <tran:provider-id>jca</tran:provider-id>
        <tran:inbound>false</tran:inbound>
        <tran:URI>
            <env:value>jca://eis/DB/cmsolicitudesXA</env:value>
        </tran:URI>
        <tran:outbound-properties>
            <tran:load-balancing-algorithm>round-robin</tran:load-balancing-algorithm>
            <tran:retry-count>0</tran:retry-count>
            <tran:retry-interval>30</tran:retry-interval>
            <tran:retry-application-errors>false</tran:retry-application-errors>
        </tran:outbound-properties>
        <tran:provider-specific xsi:type="jca:JCAEndPointConfiguration" xmlns:jca="http://www.bea.com/wli/sb/transports/jca">
            <jca:jca-file ref="OperacionCreditos/SolicitudesVSR/Resources/Adapters/NMPSolicitudesClienteDB"/>
            <jca:adapter-name>Database Adapter</jca:adapter-name>
            <jca:adapter-type>db</jca:adapter-type>
            <jca:dispatch-policy>SBDefaultResponseWorkManager</jca:dispatch-policy>
            <jca:always-use-wsdl>true</jca:always-use-wsdl>
            <jca:connection-mode>managed</jca:connection-mode>
            <jca:outbound-properties>
                <jca:operation-properties>
                    <jca:operation-name>guardarSolicitud</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>DetectOmissions</jca:name>
                            <jca:value>true</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>DescriptorName</jca:name>
                            <jca:value>NMPSolicitudesClienteDB.NmpSolicitudesClientesInfo</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>MappingsMetaDataURL</jca:name>
                            <jca:value>servicebus:/OperacionCreditos/SolicitudesVSR/Resources/Adapters/NMPSolicitudesClienteDB-MAP.xml</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>DmlType</jca:name>
                            <jca:value>insert</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>validarVehiculoActualizar</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>SqlString</jca:name>
                            <jca:value>select          sol.folio_tramite,         sol.ESTADO_SOLICITUD,         sol.id_contrato         from          NMP_SOLICITUDES_CLIENTES_INFO sol,         nmp_vehiculo_info veh         where sol.NUM_CLIENTE = veh.NUM_CLIENTE         and sol.FOLIO_TRAMITE = veh.FOLIO_TRAMITE         and veh.VIN = :vin         and sol.ESTADO_SOLICITUD in (1,2)         and sol.folio_tramite not in (:folioTramite)         and sol.NUM_CLIENTE not in (:numCliente)</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>consultarEstadoSolicitud</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>SqlString</jca:name>
                            <jca:value>SELECT  NUM_CLIENTE,  FOLIO_TRAMITE,  ESTADO_SOLICITUD,  ESTADO_FLUJO,  ID_MOTIVO_RECHAZO, FECHA_CREACION FROM NMP_SOLICITUDES_CLIENTES_INFO  WHERE (NUM_CLIENTE = :numCliente) AND trunc(FECHA_CREACION) >= nvl(:fechaInicial,trunc(FECHA_CREACION)) AND trunc(FECHA_CREACION) &lt;=  nvl(:fechaFinal,trunc(FECHA_CREACION))</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>consultarSolicitud</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>DescriptorName</jca:name>
                            <jca:value>NMPSolicitudesClienteDB.NmpSolicitudesClientesInfo</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>MappingsMetaDataURL</jca:name>
                            <jca:value>servicebus:/OperacionCreditos/SolicitudesVSR/Resources/Adapters/NMPSolicitudesClienteDB-MAP.xml</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>QueryName</jca:name>
                            <jca:value>readObject</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>consultarSolicitudesCliente</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>ReturnSingleResultSet</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>DescriptorName</jca:name>
                            <jca:value>NMPSolicitudesClienteDB.NmpSolicitudesClientesInfo</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>MappingsMetaDataURL</jca:name>
                            <jca:value>servicebus:/OperacionCreditos/SolicitudesVSR/Resources/Adapters/NMPSolicitudesClienteDB-MAP.xml</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>QueryName</jca:name>
                            <jca:value>NMPSolicitudesClienteSelect</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>asociarSolicitud</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>DetectOmissions</jca:name>
                            <jca:value>true</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>DescriptorName</jca:name>
                            <jca:value>NMPSolicitudesClienteDB.NmpSolicitudesClientesInfo</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>MappingsMetaDataURL</jca:name>
                            <jca:value>servicebus:/OperacionCreditos/SolicitudesVSR/Resources/Adapters/NMPSolicitudesClienteDB-MAP.xml</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>OptimizeMerge</jca:name>
                            <jca:value>true</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>DmlType</jca:name>
                            <jca:value>merge</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>validarVehiculo</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>SqlString</jca:name>
                            <jca:value>select          sol.folio_tramite,         sol.ESTADO_SOLICITUD,         sol.id_contrato         from          NMP_SOLICITUDES_CLIENTES_INFO sol,         nmp_vehiculo_info veh         where sol.NUM_CLIENTE = veh.NUM_CLIENTE         and sol.FOLIO_TRAMITE = veh.FOLIO_TRAMITE         and veh.VIN = :vin         and sol.ESTADO_SOLICITUD in (1,2)         and nvl(:folioTramite,1) = 1         and nvl(:numCliente,1) = 1</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>actualizarSolicitud</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>DetectOmissions</jca:name>
                            <jca:value>true</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>DescriptorName</jca:name>
                            <jca:value>NMPSolicitudesClienteDB.NmpSolicitudesClientesInfo</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>MappingsMetaDataURL</jca:name>
                            <jca:value>servicebus:/OperacionCreditos/SolicitudesVSR/Resources/Adapters/NMPSolicitudesClienteDB-MAP.xml</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>DmlType</jca:name>
                            <jca:value>update</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>eliminarSolicitud</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>DescriptorName</jca:name>
                            <jca:value>NMPSolicitudesClienteDB.NmpSolicitudesClientesInfo</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>MappingsMetaDataURL</jca:name>
                            <jca:value>servicebus:/OperacionCreditos/SolicitudesVSR/Resources/Adapters/NMPSolicitudesClienteDB-MAP.xml</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>DmlType</jca:name>
                            <jca:value>delete</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
                <jca:operation-properties>
                    <jca:operation-name>validaEstadoFlujoSolicitud</jca:operation-name>
                    <jca:spec-properties>
                        <jca:property>
                            <jca:name>SqlString</jca:name>
                            <jca:value>select count(id_etapa) from NMP_ETAPAS_FLUJO_INFO where id_etapa = (select estado_flujo from nmp_solicitudes_clientes_info where NUM_CLIENTE = #numCliente and folio_tramite = #folioTramite) and id_etapa_siguiente = #estadoFlujo</jca:value>
                        </jca:property>
                        <jca:property>
                            <jca:name>GetActiveUnitOfWork</jca:name>
                            <jca:value>false</jca:value>
                        </jca:property>
                    </jca:spec-properties>
                </jca:operation-properties>
            </jca:outbound-properties>
        </tran:provider-specific>
    </con:endpointConfig>
</con:businessServiceEntry>