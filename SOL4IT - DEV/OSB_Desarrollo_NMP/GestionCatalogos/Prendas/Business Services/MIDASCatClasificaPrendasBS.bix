<?xml version="1.0" encoding="UTF-8"?>
<con:businessServiceEntry xmlns:con="http://xmlns.oracle.com/servicebus/business/config" xmlns:tran="http://www.bea.com/wli/sb/transports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <con:coreEntry>
        <con:description>Servicio expuesto por MIDAS del cual se recupera informaci√≥n de los catalgos y subcatalogos usados para clasificar las prendas que van a concentradora.</con:description>
        <con1:binding type="SOAP" xsi:type="con:SoapBindingType" isSoap12="false" xmlns:con="http://www.bea.com/wli/sb/services/bindings/config" xmlns:con1="http://xmlns.oracle.com/servicebus/business/config">
            <con:wsdl ref="GestionCatalogos/Prendas/Resources/WSDLs/MIDASCatClasficaPrendasWSDL"/>
            <con:port>
                <con:name>CatalogoBPMWSHttpPort</con:name>
                <con:namespace>urn:mx.com.nmp.midas</con:namespace>
            </con:port>
            <con:WSI-compliant>false</con:WSI-compliant>
        </con1:binding>
        <oper:operations enabled="true" xmlns:oper="http://xmlns.oracle.com/servicebus/business/operations" xmlns:env="http://www.bea.com/wli/config/env">
            <oper:throttling enabled="false"/>
            <oper:resultCachingEnabled>true</oper:resultCachingEnabled>
        </oper:operations>
        <con:ws-policy xmlns:env="http://www.bea.com/wli/config/env">
            <ser:binding-mode xmlns:ser="http://www.bea.com/wli/sb/services">no-policies</ser:binding-mode>
        </con:ws-policy>
        <con:xqConfiguration>
            <ser:snippetVersion xmlns:ser="http://www.bea.com/wli/sb/services">1.0</ser:snippetVersion>
        </con:xqConfiguration>
    </con:coreEntry>
    <con:endpointConfig>
        <tran:provider-id>http</tran:provider-id>
        <tran:inbound>false</tran:inbound>
        <tran:URI>
            <env:value xmlns:env="http://www.bea.com/wli/config/env">http://10.30.3.55:9032/midas-web/services/CatalogoBPMWS</env:value>
        </tran:URI>
        <tran:outbound-properties>
            <tran:load-balancing-algorithm>round-robin</tran:load-balancing-algorithm>
            <tran:retry-count>0</tran:retry-count>
            <tran:retry-interval>0</tran:retry-interval>
            <tran:retry-application-errors>false</tran:retry-application-errors>
        </tran:outbound-properties>
        <tran:provider-specific xsi:type="http:HttpEndPointConfiguration" xmlns:http="http://www.bea.com/wli/sb/transports/http">
            <http:outbound-properties>
                <http:request-method>POST</http:request-method>
                <http:timeout>3</http:timeout>
                <http:connection-timeout>3</http:connection-timeout>
                <http:follow-redirects>false</http:follow-redirects>
                <http:chunked-streaming-mode>true</http:chunked-streaming-mode>
                <http:session-sctikiness enabled="false" session-id-name="JSESSIONID"/>
            </http:outbound-properties>
            <http:dispatch-policy>SBDefaultResponseWorkManager</http:dispatch-policy>
            <http:compression>
                <http:compression-support>false</http:compression-support>
            </http:compression>
        </tran:provider-specific>
    </con:endpointConfig>
    <aler:alertRules xmlns:aler="http://xmlns.oracle.com/servicebus/monitoring/alert">
        <aler:alertRule enabled="true" name="timeout_error_warning">
            <aler:frequency>once-until-conditions-clear</aler:frequency>
            <aler:severity>warning</aler:severity>
            <aler:stopProcessing>false</aler:stopProcessing>
            <aler:condition aggregation-interval="60">
                <con1:logicalExpr logical-operator="or" xmlns:con1="http://xmlns.oracle.com/servicebus/monitoring/alert/condition">
                    <con1:logicalExpr logical-operator="and">
                        <con1:monCondExpr>
                            <con1:function>average</con1:function>
                            <con1:lhs>Transport.response-time</con1:lhs>
                            <con1:operator>></con1:operator>
                            <con1:rhs>3999</con1:rhs>
                        </con1:monCondExpr>
                        <con1:monCondExpr>
                            <con1:function>average</con1:function>
                            <con1:lhs>Transport.response-time</con1:lhs>
                            <con1:operator>&lt;</con1:operator>
                            <con1:rhs>5000</con1:rhs>
                        </con1:monCondExpr>
                    </con1:logicalExpr>
                    <con1:logicalExpr logical-operator="and">
                        <con1:monCondExpr>
                            <con1:function>count</con1:function>
                            <con1:lhs>Transport.error-count</con1:lhs>
                            <con1:operator>></con1:operator>
                            <con1:rhs>9</con1:rhs>
                        </con1:monCondExpr>
                        <con1:monCondExpr>
                            <con1:function>count</con1:function>
                            <con1:lhs>Transport.error-count</con1:lhs>
                            <con1:operator>&lt;</con1:operator>
                            <con1:rhs>20</con1:rhs>
                        </con1:monCondExpr>
                    </con1:logicalExpr>
                </con1:logicalExpr>
            </aler:condition>
            <aler:alertDestination ref="GestionCatalogos/Prendas/Resources/Alerts/OSBCatalogosPrendasAlert"/>
        </aler:alertRule>
        <aler:alertRule enabled="true" name="timeout_error_major">
            <aler:frequency>once-until-conditions-clear</aler:frequency>
            <aler:severity>major</aler:severity>
            <aler:stopProcessing>false</aler:stopProcessing>
            <aler:condition aggregation-interval="60">
                <con1:logicalExpr logical-operator="or" xmlns:con1="http://xmlns.oracle.com/servicebus/monitoring/alert/condition">
                    <con1:logicalExpr logical-operator="and">
                        <con1:monCondExpr>
                            <con1:function>average</con1:function>
                            <con1:lhs>Transport.response-time</con1:lhs>
                            <con1:operator>></con1:operator>
                            <con1:rhs>4999</con1:rhs>
                        </con1:monCondExpr>
                        <con1:monCondExpr>
                            <con1:function>average</con1:function>
                            <con1:lhs>Transport.response-time</con1:lhs>
                            <con1:operator>&lt;</con1:operator>
                            <con1:rhs>10000</con1:rhs>
                        </con1:monCondExpr>
                    </con1:logicalExpr>
                    <con1:logicalExpr logical-operator="and">
                        <con1:monCondExpr>
                            <con1:function>count</con1:function>
                            <con1:lhs>Transport.error-count</con1:lhs>
                            <con1:operator>></con1:operator>
                            <con1:rhs>19</con1:rhs>
                        </con1:monCondExpr>
                        <con1:monCondExpr>
                            <con1:function>count</con1:function>
                            <con1:lhs>Transport.error-count</con1:lhs>
                            <con1:operator>&lt;</con1:operator>
                            <con1:rhs>30</con1:rhs>
                        </con1:monCondExpr>
                    </con1:logicalExpr>
                </con1:logicalExpr>
            </aler:condition>
            <aler:alertDestination ref="GestionCatalogos/Prendas/Resources/Alerts/OSBCatalogosPrendasAlert"/>
        </aler:alertRule>
        <aler:alertRule enabled="true" name="timeout_error_critical">
            <aler:frequency>once-until-conditions-clear</aler:frequency>
            <aler:severity>fatal</aler:severity>
            <aler:stopProcessing>false</aler:stopProcessing>
            <aler:condition aggregation-interval="60">
                <con1:logicalExpr logical-operator="or" xmlns:con1="http://xmlns.oracle.com/servicebus/monitoring/alert/condition">
                    <con1:monCondExpr>
                        <con1:function>average</con1:function>
                        <con1:lhs>Transport.response-time</con1:lhs>
                        <con1:operator>></con1:operator>
                        <con1:rhs>9000</con1:rhs>
                    </con1:monCondExpr>
                    <con1:monCondExpr>
                        <con1:function>count</con1:function>
                        <con1:lhs>Transport.error-count</con1:lhs>
                        <con1:operator>></con1:operator>
                        <con1:rhs>29</con1:rhs>
                    </con1:monCondExpr>
                </con1:logicalExpr>
            </aler:condition>
            <aler:alertDestination ref="GestionCatalogos/Prendas/Resources/Alerts/OSBCatalogosPrendasAlert"/>
        </aler:alertRule>
    </aler:alertRules>
</con:businessServiceEntry>