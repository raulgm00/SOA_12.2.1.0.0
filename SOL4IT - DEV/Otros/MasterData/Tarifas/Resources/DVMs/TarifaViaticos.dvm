<dvm name="TarifaViaticos" xmlns="http://xmlns.oracle.com/dvm">
  <description>Este DVM muestra los codigos, descripciones, valores activos para los viaticos de un empleado de Qualitas</description>
  <columns>
    <column name="Codigo"/>
    <column name="Nombre"/>
    <column name="Descripcion"/>
    <column name="Valor"/>
    <column name="Activa"/>
  </columns>
  <rows>
    <row>
      <cell>HOS</cell>
      <cell>Hospedaje</cell>
      <cell>Gastos de Hotel  x Día</cell>
      <cell>1500.00</cell>
      <cell>true</cell>
    </row>
    <row>
      <cell>ALI</cell>
      <cell>Alimentos</cell>
      <cell>Gastos de Alimentación x Día</cell>
      <cell>450.00</cell>
      <cell>true</cell>
    </row>
    <row>
      <cell>GEN</cell>
      <cell>Gastos Generales</cell>
      <cell>Gastos Generales x Día</cell>
      <cell>200.00</cell>
      <cell>true</cell>
    </row>
    <row>
      <cell>KM</cell>
      <cell>Kilometraje</cell>
      <cell>Kilometraje (x KMs)</cell>
      <cell>2.50</cell>
      <cell>true</cell>
    </row>
  </rows>
</dvm>